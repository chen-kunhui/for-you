{
  	"name": "for-you",
	"displayName": "for you",
	"description": "For you",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.66.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
        "*"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"customEditors": [
			{
				"viewType": "for-you.csv-editor",
				"displayName": "CSV editor",
				"selector": [
					{
						"filenamePattern": "*.csv"
					}
				],
				"priority": "option"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"compile-tests": "tsc -p . --outDir out",
		"watch-tests": "tsc -p . -w --outDir out",
		"pretest": "npm run compile-tests && npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.66.0",
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.0",
		"@types/node": "14.x",
		"@typescript-eslint/eslint-plugin": "^5.16.0",
		"@typescript-eslint/parser": "^5.16.0",
		"eslint": "^8.11.0",
		"glob": "^7.2.0",
		"mocha": "^9.2.2",
		"typescript": "^4.5.5",
		"ts-loader": "^9.2.8",
		"@vue/compiler-sfc": "^3.2.0",
		"vue": "^3.2.0",
		"vue-loader": "^16.2.0",
		"vscode-codicons": "0.0.17",
		"sass": "^1.34.1",
		"sass-loader": "^12.0.0",
		"file-loader": "^6.2.0",
		"css-loader": "^5.2.6",
		"mini-css-extract-plugin": "^1.6.0",
		"url-loader": "^4.1.1",
		"webpack": "^5.70.0",
		"webpack-cli": "^4.9.2",
		"@vscode/test-electron": "^2.1.3"
	}
}
